<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : permisosmarca.xml
    Author     : Blas stanciuc
    Description:
        Structure for the users table.
-->
<table>
    <column>
        <name>codpermisossubfamilia</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>idusuariosubfamilia</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>idsubfamilia</name>
        <type>integer</type>
    </column>
    <constraint>
        <name>permisos_pkey</name>
        <type>PRIMARY KEY (codpermisossubfamilia)</type>
    </constraint>
        <constraint>
        <name>f_idusuariosubfamilia</name>
        <type>FOREIGN KEY (idusuariosubfamilia) REFERENCES users (nick) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>f_idsubfamilia</name>
        <type>FOREIGN KEY (idsubfamilia) REFERENCES subfamilias (id) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>uniq_user_subfamilia</name>
        <type>UNIQUE (idusuariosubfamilia,idsubfamilia)</type>
    </constraint>
</table>
